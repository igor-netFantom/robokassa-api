{
    "name": "netfantom/robokassa-api",
    "type": "library",
    "description": "Robokassa Api for PHP 8.1",
    "keywords": [
        "robokassa",
        "api",
        "payment"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Fedorov",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "netFantom\\RobokassaApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^5.12"
    },
    "config": {
        "sort-packages": true
    }
}
